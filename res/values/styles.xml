<?xml version="1.0" encoding="UTF-8"?>
<resources>
	
	<style name="SketchbookButton">
		<item name="android:padding">@dimen/padding</item>
		<item name="android:layout_width">@dimen/icon</item>
		<item name="android:layout_height">@dimen/icon</item>
	</style>
	<style name="SketchbookSplash">
		<item name="android:layout_width">@dimen/splash</item>
		<item name="android:layout_height">@dimen/splash</item>
	</style>
	<style name="SketchbookLabel">
		<item name="android:padding">@dimen/padding</item>
		<item name="android:layout_width">@dimen/label</item>
		<item name="android:layout_height">@dimen/desktop</item>
	</style>
	<style name="PlouikGridView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">@dimen/gridView</item>
		<item name="android:numColumns">auto_fit</item>
		<item name="android:columnWidth">@dimen/preset</item>
		<item name="android:stretchMode">columnWidth</item>
		<item name="android:verticalSpacing">1px</item>
		<item name="android:horizontalSpacing">1px</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="SketchbookPatternGridView">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:numColumns">auto_fit</item>
		<item name="android:columnWidth">@dimen/icon</item>
		<item name="android:stretchMode">columnWidth</item>
		<item name="android:verticalSpacing">5px</item>
		<item name="android:horizontalSpacing">1px</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="SketchbookFileGridView">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">@dimen/gridView</item>
		<item name="android:numColumns">auto_fit</item>
		<item name="android:columnWidth">@dimen/preset</item>
		<item name="android:stretchMode">columnWidth</item>
		<item name="android:verticalSpacing">5sp</item>
	</style>
	<style name="SketchbookSeekbar">
		<item name="android:layout_width">@dimen/seekbarWidth</item>
		<item name="android:layout_height">@dimen/seekbar</item>
	</style>
	<style name="SketchbookSeekbarHalf">
		<item name="android:layout_width">@dimen/icon</item>
		<item name="android:layout_height">@dimen/seekbar</item>
	</style>
	<style name="SketchbookSeekbarColor">
		<item name="android:layout_width">@dimen/seekbarColor</item>
		<item name="android:layout_height">@dimen/seekbar</item>
		<item name="android:layout_gravity">center</item>
	</style>
	<style name="SketchbookTextSmall">
		<item name="android:textSize">@dimen/textSizeSmall</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="SketchbookTextOverview">
		<item name="android:textSize">@dimen/textSizeSmall</item>
	</style>
	<style name="SketchbookTextBrush">
		<item name="android:textSize">@dimen/textSizeNormal</item>
		<item name="android:padding">5px</item>
	</style>
	<style name="SketchbookTextTools">
		<item name="android:textSize">@dimen/textSizeNormal</item>
		<item name="android:textColor">@android:color/white</item>
		<item name="android:padding">5px</item>
	</style>
	<style name="SketchbookTextColor">
		<item name="android:textSize">@dimen/textSizeSmall</item>
		<item name="android:padding">2px</item>
	</style>
	<style name="SketchbookPrompt">
		<item name="android:textColor">@android:color/white</item>
		<item name="android:background">@android:color/black</item>
		<item name="android:textSize">@dimen/textSizeNormal</item>
		<item name="android:lines">1</item>
	</style>
	<style name="SketchbookDialog">
		<item name="android:padding">@dimen/padding</item>
		<item name="android:background">@android:color/white</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="SketchbookDialogBorder">
		<item name="android:padding">1px</item>
		<item name="android:background">@android:color/black</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="SketchbookSpinner">
		<item name="android:background">@android:color/black</item>
		<item name="android:textColor">@android:color/white</item>
	</style>
	<style name="SketchbookSpinnerLarge">
		<item name="android:background">@android:color/black</item>
		<item name="android:textColor">@android:color/white</item>
		<item name="android:textSize">18pt</item>
		<item name="android:padding">18pt</item>
	</style>
	<style name="SketchbookMenu">
		<item name="android:padding">2px</item>
		<item name="android:layout_width">@dimen/preset</item>
		<item name="android:layout_height">@dimen/preset</item>
	</style>
	<style name="SketchbookDesktop">
		<item name="android:padding">2px</item>
		<item name="android:layout_width">@dimen/desktop</item>
		<item name="android:layout_height">@dimen/desktop</item>
	</style>
	<style name="PlouikLastColors">
		<item name="android:layout_width">@dimen/icon</item>
		<item name="android:layout_height">@dimen/desktop</item>
		<item name="android:layout_gravity">center</item>
	</style>
	<style name="SketchbookPresetOption">
		<item name="android:padding">1px</item>
		<item name="android:layout_width">@dimen/presetoption</item>
		<item name="android:layout_height">@dimen/presetoption</item>
	</style>
</resources>

